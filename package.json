{
  "name": "gestion-de-eventos",
  "version": "0.1.0",
  "description": "Sistema profesional de gestión de eventos empresariales",
  "private": true,
  "author": {
    "name": "Manuel Antonio Tut Solorzano",
    "email": "soporteapps@hexalux.mx",
    "company": "MATS Hexalux"
  },
  "keywords": [
    "gestión de eventos",
    "reservas",
    "cotizaciones",
    "calendario",
    "next.js",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev --port 3200",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "db:demo": "tsx scripts/demo-data.ts",
    "db:studio": "prisma studio",
    "init:local": "npx prisma generate && npx prisma migrate reset --force && npm run db:seed",
    "init:local:push": "npx prisma db push --force-reset && npm run db:seed",
    "seed:sandbox": "node scripts/seed-runner.js sandbox",
    "seed:prod": "node scripts/seed-runner.js prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "prepare": "husky install",
    "release": "standard-version",
    "lint-staged": "lint-staged",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.0",
