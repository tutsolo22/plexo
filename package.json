    "@upstash/redis": "^1.35.6",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^2.30.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.294.0",
    "next": "^14.2.0",
    "next-auth": "5.0.0-beta.22",
    "nodemailer": "^6.9.7",
    "openai": "^6.3.0",
    "react": "^18.3.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.47.0",
